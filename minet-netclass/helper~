./stop_minet.sh
make clean
make
cd bin
rm device_driver2
rm reader
rm writer
ln -s /usr/local/eecs340/device_driver2
ln -s /usr/local/eecs340/reader
ln -s /usr/local/eecs340/writer
cd ..
chmod 777 proj2init.sh
./setup.sh
cd fifos
chmod a+w ether2mon
chmod a+w ether2mux
cd ..
cp minet_safe.cfg minet.cfg
./start_minet.sh "udp_server u 5050"
